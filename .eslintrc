{
	"env": {
		"browser": true
	},
	"extends": ["prettier", "plugin:@typescript-eslint/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"ignorePatterns": ["*.js", "dist", "demo/dist"],
	"rules": {
		"no-console": "error"
	}
}
