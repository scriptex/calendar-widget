{
	"name": "simple-calendar-widget",
	"version": "0.1.0",
	"description": "A super simple calendar widget written in TypeScript and SCSS",
	"scripts": {
		"sass": "node-sass main.scss dist/index.css",
		"postcss": "postcss dist/index.css -o dist/index.css -c postcss.config.js",
		"css": "yarn sass && yarn postcss",
		"js": "tsc -p .",
		"lint": "tslint -p tsconfig.json -c tslint.json index.ts",
		"build": "yarn js && yarn css"
	},
	"keywords": [
		"Calendar",
		"Widget",
		"TypeScript",
		"SCSS"
	],
	"author": "Atanas Atanasov <scriptex.bg@gmail.com> (https://atanas.info)",
	"repository": "git@github.com:scriptex/calendar-widget.git",
	"bugs": {
		"url": "https://github.com/scriptex/calendar-widget/issues",
		"email": "scriptex.bg@gmail.com"
	},
	"license": "MIT",
	"main": "dist/index.js",
	"style": "dist/index.css",
	"dependencies": {},
	"devDependencies": {
		"autoprefixer": "9.8.6",
		"node-sass": "4.14.1",
		"postcss-cli": "7.1.1",
		"postcss-flexbugs-fixes": "4.2.1",
		"tslint": "6.1.3",
		"tslint-config-prettier": "1.18.0",
		"typescript": "3.9.7"
	}
}
